  isPayoutDaily                      Boolean?  @map("is_payout_daily")
  isSalesInserted                    Boolean?  @map("is_sales_inserted")
  isTestBonusGiven                   Boolean?  @map("is_test_bonus_given")
  isTestBonusExpired                 Boolean?  @map("is_test_bonus_expired")
  isValidDeposited                   Boolean?  @map("is_valid_deposited")
  userName                           String?   @map("user_name")
  taskProgress                       Json?     @map("task_progress")
  taskDetails                        Json?     @map("task_details")
  recommendCustomers                 Json?     @map("recommend_customers")
  lastSalesRecords                   Json?     @map("last_sales_records")
  lastPayoutRecords                  Json?     @map("last_payout_records")
  wallets                            Json?
  totalRowsCount                     Int?      @map("total_rows_count")
  user                               User?     @relation(fields: [userID], references: [id])
  referrer                           Customer? @relation("CustomerReferrer", fields: [referrerCustomerID], references: [id])
  referrals                          Customer[] @relation("CustomerReferrer")
  ambassadorLevelRef                 AmbassadorLevel? @relation(fields: [ambassadorLevelID], references: [id])
  bankDetails                        CustomerBankDetail[]
  depositRecords                     DepositRecord[]
  withdrawals                        Withdrawal[]
  transactionRecords                 TransactionRecord[]
  customerTasks                      CustomerTask[]
